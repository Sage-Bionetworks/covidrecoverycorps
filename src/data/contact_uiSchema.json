{
  "data": {
    "classNames": "contact",
    "firstName": {
      "ui:placeholder": "First Name",
      "classNames": "firstName text"
    },
    "lastName": {
      "ui:placeholder": "Last Name",
      "classNames": "lastName text"
    },

    "attributes": {
      "address1": {
        "ui:placeholder": "Street Address",
        "classNames": "address1 text"
      },
      "address2": {
        "classNames": "address2 text",
        "ui:placeholder": "Apt/Suite/Other"
      },
      "zip_code": {
        "classNames": "zip",
        "ui:placeholder": "Zip code (5-digit)"
      },
      "state": {
        "classNames": "state text",
        "ui:placeholder": "State"
      },
      "city": {
        "classNames": "city text",
        "ui:placeholder": "City"
      },

      "gender": {
        "classNames": "gender",
        "ui:widget": "radio",
        "ui:options": {
          "inline": false
        }
      },

      "dob": {
        "classNames": "birthdate text",
        "ui:description": "Your birthdate helps us ensure we match the biosamples to the right person.",
        "ui:widget": "CustomDateWidget",
        "ui:options": {
          "yearsRange": [1910, 2003],
          "hideNowButton": true,
          "hideClearButton": true
        }
      }
    }
  }
}
