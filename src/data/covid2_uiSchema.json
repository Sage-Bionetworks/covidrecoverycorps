        "travel_start": {
          "ui:widget": "CustomDateWidget",
          "ui:options": {
            "yearsRange": [2020, 2021],
            "hideNowButton": true,
            "hideClearButton": true,
            "lessThanNow": true
          }
        },
        "travel_end": {
          "ui:widget": "CustomDateWidget",
          "ui:options": {
            "yearsRange": [2020, 2021],
            "hideNowButton": true,
            "hideClearButton": true,
            "lessThanNow": true
          }
        }
      }
    },
    "household_size": {
      "classNames": "show_fieldSet household_size",
