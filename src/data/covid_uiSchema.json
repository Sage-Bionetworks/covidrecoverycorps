    "symptoms_dates": {
      "classNames": "subfields",
      "ui:order": ["first_day", "last_day", "still_have_symptoms"],
      "first_day": {
        "ui:widget": "CustomDateWidget",
        "ui:options": {
          "yearsRange": [2020, 2021],
