      "ui:options": {
        "inline": false
      },
      "nasal_swab_date": {
        "classNames": ["nasal_swab"],
        "ui:widget": "CustomDateWidget",
        "ui:options": {
          "yearsRange": [2020, 2021],
          "hideNowButton": true,
          "hideClearButton": true,
          "lessThanNow": true
        }
      },
      "nasal_swab_result": {
        "ui:widget": "radio",
        "classNames": ["secondary_radio test_result subcheckbox"],
        "ui:options": {
          "inline": false
        }
      },
      "serum_test_date": {
        "classNames": ["nasal_swab"],
        "ui:widget": "CustomDateWidget",
        "ui:options": {
          "yearsRange": [2020, 2021],
          "hideNowButton": true,
          "hideClearButton": true,
          "lessThanNow": true
