          "hideClearButton": true,
          "lessThanNow": true
        },
        "classNames": ["symptom_day_first"]
      },
      "last_day": {
        "ui:widget": "CustomDateWidget",
        "ui:options": {
          "yearsRange": [2020, 2021],
          "hideNowButton": true,
          "hideClearButton": true,
          "lessThanNow": true
