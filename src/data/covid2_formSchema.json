    "country": {
      "type": "object",
      "properties": {
        "country_name": {
          "title": "Name of the country",
          "type": "string",
          "enum": {
            "$ref": "#/definitions/countryList"
          },
          "enumNames": {
            "$ref": "#/definitions/countryListNames"
          }
        },
        "travel_start": {
          "title": "From:",
          "type": "string"
        },
        "travel_end": {
          "title": "To:",
          "type": "string"
        }
      }
    },
    "covid1": {
      "type": "object",
      "title": "",
      "required": ["travel", "household_size", "housing_type"],
      "properties": {
        "travel": {
          "type": "string",
          "title": "1. Were you abroad just before getting sick/being exposed to or being suspected for COVID-19 infection?",
          "enum": ["No", "Yes", "Prefer not to answer"],
          "enumNames": ["No", "Yes", "Prefer not to answer"]
        },
        "household_size": {
          "type": "object",
          "title": "2. How many people (including yourself) live in your apartment/house in each age category? (Fill in 0 if none are)",

          "properties": {
            "under3": {
              "type": "number",
              "minimum": 0,
              "default": 0,
              "title": "under 3 years old"
            },
            "3-6": {
              "type": "number",
              "minimum": 0,
              "default": 0,
              "title": "3-6 years old"
            },
            "7–17": {
              "type": "number",
              "minimum": 0,
              "default": 0,
              "title": "7–17 years old"
            },
            "18–64": {
              "type": "number",
              "minimum": 0,
              "default": 0,
              "title": "18–64 years old"
            },
            "65–79": {
              "type": "number",
              "minimum": 0,
              "default": 0,
              "title": "65–79 years old"
            },
            "80+": {
              "type": "number",
              "minimum": 0,
              "default": 0,
              "title": "80 years old or older"
            },
            "no_answer": {
              "type": "boolean",

              "title": "Prefer not to answer"
            }
          }
        },
        "housing_type": {
          "type": "string",
          "title": "3. What type of household do you live in?",
          "uniqueItems": true,
          "enum": [
            "Studio apartment",
            "One-bedroom apartment",
            "Two-bedroom apartment",
            "Three-bedroom (or more) apartment",
            "Townhouse",
            "Free-standing house",
            "Nursing home, or rehab facility",
            "Homeless",
            "Prefer not to answer"
          ],
          "enumNames": [
            "Studio apartment",
            "One-bedroom apartment",
            "Two-bedroom apartment",
            "Three-bedroom (or more) apartment",
            "Townhouse",
            "Free-standing house",
            "Nursing home, or rehab facility",
            "Homeless",
            "Prefer not to answer"
          ]
        }
      },
      "dependencies": {
        "travel": {
          "oneOf": [
            {
              "properties": {
                "travel": {
                  "enum": ["No"]
                }
              }
            },
            {
              "properties": {
                "travel": {
                  "enum": ["Prefer not to answer"]
                }
              }
            },
            {
              "properties": {
                "travel": {
                  "enum": ["Yes"]
                },
                "travel_countries": {
                  "type": "array",
                  "title": "1b. What specific country/countries did you travel to?",
                  "minItems": 1,
                  "items": {
                    "$ref": "#/definitions/country"
                  }
                }
              }
            }
          ]
        }
      }
    },
    "covid11": {
      "type": "object",
      "title": "",
      "required": ["people_diagnosed", "antibiotics_covid_prior"],
      "properties": {
        "people_infected": {
          "type": "number",
          "title": "4. How many people in your household have been infected with or presumed to be infected with COVID-19?"
        },
        "people_diagnosed": {
          "title": "5. How many of your immediate or extended family members related by blood have the following:",
          "type": "object",
          "minProperties": 1,
          "properties": {
            "covid_diagnosed": {
              "type": "number",
              "title": "Been diagnosed (have a positive lab test result) with COVID-19?",
              "default": 0,
              "minimum": 0
            },
            "covid_suspected": {
              "type": "number",
              "title": "Had symptoms but no positive test result for COVID-19?",
              "default": 0,
              "minimum": 0
            },
            "covid_hospitalized": {
              "type": "number",
              "title": "Been hospitalized due to COVID-19?",
              "default": 0,
              "minimum": 0
            },
            "covid_died": {
              "type": "number",
              "title": "Died due to COVID-19?",
              "default": 0,
              "minimum": 0
            },
            "no_answer": {
              "type": "boolean",
              "title": "Prefer not to answer"
            }
          }
        },
        "antibiotics_covid_prior": {
          "title": "6. Prior to COVID-19, when was the last time a doctor gave you antibiotics?",
          "type": "string",
          "uniqueItems": true,
          "enum": [
            "In the last month",
            "In the last year",
            "Within the last 5 years",
            "Within the last 10 years",
            "Over 10 years ago",
            "Never",
            "Prefer not to answer"
          ],
          "enumNames": [
            "In the last month",
            "In the last year",
            "Within the last 5 years",
            "Within the last 10 years",
            "Over 10 years ago",
            "Never",
            "Prefer not to answer"
          ]
        }
      }
    },
    "antibiotics": {
      "type": "object",
      "title": "",
      "required": ["antibiotics_purpose", "antibiotics_course"],
      "properties": {
        "antibiotics_purpose": {
          "title": "6a. For what purpose, did the doctor give you antibiotics the last time you took antibiotics?",
          "type": "string",
          "uniqueItems": true,
          "enum": [
            "Respiratory infection (E.G. Strep throat, sinusitis, bronchitis, pneumonia)",
            "Gastroenteritis, which caused diarrhea and/or vomiting",
            "Urinary tract infection",
            "Infection of the skin or a wound infection",
            "Other purpose",
            "Don’t know",
            "Prefer not to answer"
          ],
          "enumNames": [
            "Respiratory infection (E.G. Strep throat, sinusitis, bronchitis, pneumonia)",
            "Gastroenteritis, which caused diarrhea and/or vomiting",
            "Urinary tract infection",
            "Infection of the skin or a wound infection",
            "Other purpose",
            "Don’t know",
            "Prefer not to answer"
          ]
        },
        "antibiotics_course": {
          "title": "6b. How long was your course of antibiotics?",
          "type": "string",
          "uniqueItems": true,
          "enum": [
            "3 days",
            "7 days",
            "14 days",
            "More than 14 days",
            "I don’t know",
            "Prefer not to answer"
          ],
          "enumNames": [
            "3 days",
            "7 days",
            "14 days",
            "More than 14 days",
            "I don’t know",
            "Prefer not to answer"
          ]
        }
      },
      "dependencies": {}
    },
