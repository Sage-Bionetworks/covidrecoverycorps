{
  "data": {
    "classNames": "contact",
    "firstName": {
      "ui:placeholder": "Nombre",
      "classNames": "firstName text"
    },
    "lastName": {
      "ui:placeholder": "Apellido",
      "classNames": "lastName text"
    },

    "attributes": {
      "address1": {
        "ui:placeholder": "Dirección Física",
        "classNames": "address1 text"
      },
      "address2": {
        "classNames": "address2 text",
        "ui:placeholder": "Apartamento/Suite/Otro"
      },
      "zip_code": {
        "classNames": "zip",
        "ui:placeholder": "Código postal (5 dígitos)"
      },
      "state": {
        "classNames": "state text",
        "ui:placeholder": "Estado"
      },
      "city": {
        "classNames": "city text",
        "ui:placeholder": "Cuidad"
      },

      "gender": {
        "classNames": "gender",
        "ui:widget": "radio",
        "ui:options": {
          "inline": false
        }
      },

      "dob": {
        "classNames": "birthdate text",
        "ui:description": "Su fecha de nacimiento nos ayuda ​a asegurar que las muestras biológicas correspondan a la persona correcta.",
        "ui:widget": "CustomDateWidget",
        "ui:options": {
          "yearsRange": [1910, 2004],
          "hideNowButton": true,
          "hideClearButton": true
        }
      }
    }
  }
}
